{
  "name": "frontend",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite dev --open",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --no-tsconfig --ignore src/lib/generated/**,src/stories/**",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --write src/",
    "lint": "eslint src/",
    "hygiene": "npm run check && npm run lint && npm run format",
    "hygiene:fix": "npm run format && npm run lint -- --fix",
    "test:unit": "vitest",
    "test": "npm run test:unit -- --run && npm run test:e2e",
    "test:e2e": "playwright test",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "machine-translate": "inlang machine translate --project project.inlang",
    "api": "node scripts/generate-api.js",
    "update-config": "node scripts/update-openapitools.js",
    "postinstall": "npm run update-config && npm run generate-api || echo 'Backend not running, skipping API generation'"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4",
    "@eslint/compat": "^1.2.5",
    "@eslint/js": "^9.18.0",
    "@inlang/cli": "^3.0.0",
    "@inlang/paraglide-js": "2.0.13",
    "@openapitools/openapi-generator-cli": "^2.13.4",
    "@playwright/test": "^1.49.1",
    "@storybook/addon-a11y": "^9.1.3",
    "@storybook/addon-docs": "^9.0.5",
    "@storybook/addon-svelte-csf": "^5.0.3",
    "@storybook/addon-vitest": "^9.1.3",
    "@storybook/sveltekit": "^9.0.5",
    "@sveltejs/adapter-auto": "^6.1.0",
    "@sveltejs/adapter-vercel": "^5.6.3",
    "@sveltejs/kit": "^2.16.0",
    "@sveltejs/vite-plugin-svelte": "^5.0.0",
    "@tailwindcss/forms": "^0.5.9",
    "@tailwindcss/typography": "^0.5.15",
    "@tailwindcss/vite": "^4.0.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/svelte": "^5.2.4",
    "@types/node": "^20",
    "@vitest/browser": "3.2.2",
    "@vitest/coverage-v8": "3.2.2",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-storybook": "^9.0.5",
    "eslint-plugin-svelte": "^3.0.0",
    "globals": "^16.0.0",
    "jsdom": "^26.0.0",
    "mdsvex": "^0.12.3",
    "playwright": "^1.55.0",
    "prettier": "^3.4.2",
    "prettier-plugin-svelte": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "storybook": "^9.0.5",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "tailwindcss": "^4.0.0",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.20.0",
    "vite": "^6.2.6",
    "vitest": "^3.0.0"
  },
  "dependencies": {
    "@inlang/paraglide-js": "^2.0.0",
    "@stripe/stripe-js": "^7.9.0",
    "flowbite": "^3.1.2",
    "jwt-decode": "^4.0.0"
  }
}
